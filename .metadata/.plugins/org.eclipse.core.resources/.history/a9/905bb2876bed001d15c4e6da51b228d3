package data_structures;



public class LinkedListClass {
	class LinkedList{
		//Creating a Node
		Node head;
		
		static class Node{
			int value;
			Node next;
			
			public Node(int d) {
				value = d;
				next = null;
			}
		}
		
	}
	
	public static void main(String[] args) {
		
		LinkedList linkedList = new LinkedList();
		
		linkedList.head = new Node(1);
	}
	

}
