package data_structures;


public class LinkedListClass {
	
	Node head;
	
	 class Node{
		int value;
		Node next;
		
		public Node(int d) {
			value = d;
			next = null;
		}
	}
	
	public static void main(String[] args) {
		
		LinkedListClass linkedList = new LinkedListClass();
		
		linkedList.head = new Node(1); 
	}
	

}
