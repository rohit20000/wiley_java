package data_structures;

public class DijkstraAlgorithm {
	public static void main(String[] args) {
		int[][] graph = {
				{0, 4, 2, 0, 0},
				{1, 0, 1, 5, 0},
				{2, 1, 0, 8, 10},
				{0, 5, 8, 0, 2},
				{0, 0, 10, 2, 0}
		};
		
		int source = 0; // source vertex
		int[] distance = dijkstra(graph, source);
	}
}
